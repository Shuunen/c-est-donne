{
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "cSpell.words": [
    "Abdelrahman",
    "blobby",
    "compat",
    "Dohm",
    "Imej",
    "intlify",
    "sonarjs",
    "tznr",
